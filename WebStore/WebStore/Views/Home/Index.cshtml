@model IEnumerable<EmployeeView>
@{ 
    ViewData["Title"] = "Home";
}
<div class="container">
    <table class="table table-bordered">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.SurName</td>
                <td>@item.Patronymic</td>
                <td>@item.Age</td>
                <td>@Html.ActionLink("Детали", "Details", new { id = item.Id })</td>
            </tr>
        }
    </table>
</div>
@await Html.PartialAsync("Partial/_Slider")
<section>
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                @await Html.PartialAsync("Partial/_LeftSideBar")
            </div>
            <div class="col-sm-9 padding-right">
                @await Html.PartialAsync("Partial/_FeaturedItems")
                @await Html.PartialAsync("Partial/_CategoryTab")
                @await Html.PartialAsync("Partial/_RecommendedItems")
            </div>
        </div>
    </div>
</section>